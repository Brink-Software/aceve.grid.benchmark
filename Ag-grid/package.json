{
  "name": "ag-grid-crud",
  "version": "1.0.0",
  "description": "AG Grid met 200.000 rijen, 500 kolommen, row grouping en CRUD functionaliteit",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "build:wasm": "powershell -ExecutionPolicy Bypass -File build-wasm.ps1",
    "build:wasm:unix": "bash build-wasm.sh",
    "build:all": "npm run build:wasm && npm run build",
    "watch": "tsc --watch",
    "dev": "tsc --watch",
    "clean": "rm -rf dist wasm",
    "serve": "npx serve -l 8000",
    "test": "cd ../playwright && npm run test:ag-grid",
    "test:ui": "cd ../playwright && npm run test:ag-grid:ui",
    "test:headed": "cd ../playwright && npm run test:ag-grid:headed"
  },
  "keywords": [
    "ag-grid",
    "typescript",
    "row-grouping",
    "crud",
    "large-dataset"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@playwright/test": "^1.40.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ag-grid-community": "^31.0.0",
    "ag-grid-enterprise": "^34.3.1"
  }
}
