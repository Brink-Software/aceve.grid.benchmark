  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@grapecity/wijmo.all": "^5.20252.42",
    "ag-grid-community": "^31.0.0",
    "ag-grid-enterprise": "^34.3.1",
    "tabulator-tables": "^6.3.1"
  }
}
