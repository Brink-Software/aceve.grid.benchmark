    "test:continuous": "powershell -ExecutionPolicy Bypass -File ./run-continuous-tests.ps1"
  },
  "keywords": [
    "ag-grid",
    "typescript",
    "row-grouping",
    "crud",
    "large-dataset",
    "benchmark"
